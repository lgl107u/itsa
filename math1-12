#include <stdio.h>
#include <stdlib.h>

int main()
{
    float x;
    while(scanf("%f",&x)!=EOF)
    {
        int detik=0;
        float frsp = 0.762;
        float mesp = 0;
        while(x>mesp)
        {
            x += frsp;
            mesp += 1; 
            detik++;
        }
        printf("%d\n",detik);
    }
    
    return 0;
}
