#include <stdio.h>   
#include <string.h>   
#include <stdlib.h> 

int mc(int inp)
{
    if(inp>100) return (inp-10);
    else return(mc(mc(inp+11)));
}
int main(void)   
{   
    int idx,inp,otp;
    scanf("%d",&idx);
    if(idx<1 || idx>10) return 0;
    for(int i=0; i<idx;i++)
    {
        scanf("%d",&inp);
        otp = mc(inp);
        printf("%d\n",otp);
    }
    return 0;   
}
