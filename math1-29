#include <stdio.h>

int main()
{
    int x,grt,flag,i,j;

    while(scanf("%d",&x)!=EOF)
    {
        x--;
        flag =0 ;

        for(i=x;i>=0;i--)
        {
            for(j=2; j<i ; j++)
            {
                if(i%j==0) break;
            }
            if(j==i)
            {
                break;
            }
        }
        printf("%d\n",i);
    }
    return 0;
}
